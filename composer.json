{
  "name": "flavioheleno/interim",
  "description": "An open_basedir aware wrapper for tmpfile() and sys_get_temp_dir()",
  "license": "MIT",
  "keywords": [
    "open_basedir",
    "sys_get_temp_dir",
    "tmp",
    "tmpfile",
    "temporary",
    "temporary directory",
    "temporary file"
  ],
  "authors": [
    {
      "name": "Flavio Heleno",
      "email": "flaviohbatista@gmail.com",
      "homepage": "https://flavioheleno.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Interim\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Interim\\Test\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^3.7"
  }
}
